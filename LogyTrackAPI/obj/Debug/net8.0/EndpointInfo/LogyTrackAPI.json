  "components": {
    "schemas": {
      "Driver": {
        "required": [
          "createdDate",
          "fullName",
          "isActive",
          "licenseNumber",
          "phoneNumber",
          "updatedDate"
        ],
        "type": "object",
        "properties": {
          "driverId": {
            "type": "integer",
            "format": "int32"
          },
          "fullName": {
            "maxLength": 100,
            "minLength": 3,
            "type": "string"
          },
          "phoneNumber": {
            "maxLength": 20,
            "minLength": 0,
            "pattern": "^[0-9\\-\\+\\(\\)\\s]+$",
            "type": "string"
          },
          "licenseNumber": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "isActive": {
            "type": "boolean"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          },
          "updatedDate": {
            "type": "string",
            "format": "date-time"
          },
          "vehicles": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Vehicle"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Product": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "integer",
            "format": "int32"
          },
          "productName": {
            "type": "string",
            "nullable": true
          },
          "sku": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "unitPrice": {
            "type": "number",
            "format": "double"
          },
          "vehicleId": {
            "type": "integer",
            "format": "int32"
          },
          "driverId": {
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          },
          "updatedDate": {
            "type": "string",
            "format": "date-time"
          },
          "vehicleNumber": {
            "type": "string",
            "nullable": true
          },
          "vehicleModel": {
            "type": "string",
            "nullable": true
          },
          "driverName": {
            "type": "string",
            "nullable": true
          },
          "driverPhone": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "pass": {
            "type": "string",
            "nullable": true
          },
          "type": {
            "type": "string",
            "nullable": true
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "Vehicle": {
        "required": [
          "capacityKg",
          "createdDate",
          "model",
          "status",
          "updatedDate",
          "vehicleNumber"
        ],
        "type": "object",
        "properties": {
          "vehicleId": {
            "type": "integer",
            "format": "int32"
          },
          "vehicleNumber": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "model": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "capacityKg": {
            "minimum": 0.01,
            "type": "number",
            "format": "double"
          },
          "driverId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "status": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          },
          "updatedDate": {
            "type": "string",
            "format": "date-time"
          },
          "driver": {
            "$ref": "#/components/schemas/Driver"
          }
        },
        "additionalProperties": false
      }
    }
  }
